{
  "name": "@zarish/mobile-app",
  "version": "1.0.0",
  "description": "ZarishCare Mobile App - React Native application for humanitarian healthcare field operations",
  "private": true,
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace ZarishMobile.xcworkspace -scheme ZarishMobile -configuration Release -archivePath build/ZarishMobile.xcarchive archive",
    "bundle:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "bundle:ios": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/main.jsbundle",
    "postinstall": "react-native-vector-icons-link",
    "clean": "react-native clean",
    "reset-cache": "react-native start --reset-cache"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.72.7",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/drawer": "^6.6.6",
    "@react-navigation/material-top-tabs": "^6.6.5",
    "react-native-screens": "^3.27.0",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-gesture-handler": "^2.13.4",
    "react-native-reanimated": "^3.6.1",
    "@tanstack/react-query": "^5.8.4",
    "axios": "^1.6.2",
    "react-native-vector-icons": "^10.0.3",
    "react-native-paper": "^5.11.6",
    "react-native-elements": "^3.4.3",
    "react-native-svg": "^14.0.0",
    "react-native-chart-kit": "^6.12.0",
    "react-native-maps": "^1.8.0",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-camera": "^4.2.1",
    "react-native-image-picker": "^7.0.3",
    "react-native-document-picker": "^9.1.1",
    "react-native-pdf": "^6.7.3",
    "react-native-print": "^0.12.0",
    "react-native-share": "^10.0.2",
    "react-native-qrcode-scanner": "^1.5.5",
    "react-native-barcode-builder": "^2.0.0",
    "react-native-device-info": "^10.11.0",
    "react-native-network-info": "^5.2.1",
    "react-native-netinfo": "^11.2.1",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-community/netinfo": "^11.2.1",
    "react-native-keychain": "^8.1.3",
    "react-native-biometrics": "^3.0.1",
    "react-native-push-notification": "^8.1.1",
    "@react-native-firebase/app": "^18.6.1",
    "@react-native-firebase/messaging": "^18.6.1",
    "react-native-background-sync": "^2.0.6",
    "react-native-sqlite-storage": "^6.0.1",
    "react-native-fs": "^2.20.0",
    "pouchdb-react-native": "^8.0.0",
    "pouchdb-adapter-asyncstorage": "^8.0.0",
    "react-native-permissions": "^4.1.0",
    "react-native-localize": "^3.0.3",
    "i18next": "^23.7.6",
    "react-i18next": "^13.5.0",
    "react-native-voice": "^3.2.4",
    "react-native-tts": "^4.1.0",
    "react-native-sound": "^0.11.2",
    "react-hook-form": "^7.48.2",
    "@hookform/resolvers": "^3.3.2",
    "yup": "^1.3.3",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21",
    "react-native-toast-message": "^2.1.7",
    "react-native-modal": "^13.0.1",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-animatable": "^1.3.3",
    "lottie-react-native": "^6.4.1",
    "@zarish/shared-types": "workspace:*"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "@babel/runtime": "^7.23.4",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@types/react": "^18.2.39",
    "@types/react-test-renderer": "^18.0.7",
    "@types/lodash": "^4.14.200",
    "babel-jest": "^29.7.0",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "0.77.0",
    "prettier": "^3.1.0",
    "react-test-renderer": "18.2.0",
    "typescript": "5.2.2",
    "@testing-library/react-native": "^12.4.2",
    "@testing-library/jest-native": "^5.4.3",
    "detox": "^20.13.5",
    "flipper-plugin-react-query-native-devtools": "^5.0.0"
  },
  "resolutions": {
    "@types/react": "^18.2.39"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|react-native-vector-icons)/)"
    ]
  },
  "keywords": [
    "react-native",
    "mobile",
    "healthcare",
    "humanitarian",
    "offline-first",
    "clinical",
    "patient-care",
    "field-operations",
    "emergency-response"
  ],
  "author": "ZarishSphere Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "react-native": {
    "zlib": "react-native-level-fs",
    "console": "react-native-console-time-polyfill",
    "process": "react-native-process"
  }
}